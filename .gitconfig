[user]
    useConfigOnly = true
[core]
    excludesfile = ~/Dotfiles/.gitignore_personal
    attributesfile = ~/Dotfiles/.gitattributes_personal
    autocrlf = input
    symlinks = true
	untrackedCache = true
	fsmonitor = true
[gc]
	auto = 256
[rerere]
	enabled = true
[pull]
	rebase = true
[merge]
	conflictstyle = zdiff3
[diff]
	algorithm = histogram
	submodule = log
[status]
	submoduleSummary = true
